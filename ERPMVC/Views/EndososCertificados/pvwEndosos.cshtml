                                                                         //var NoCD = $("#NoCD").data("kendoNumericTextBox");
                                                                         //NoCD.value(data.NoCD);
                                                                         //NoCD.trigger('change');
                                                                         $("#NoCD").val(data.NoCD);

                                                                         hcustom = data.CustomerId;
